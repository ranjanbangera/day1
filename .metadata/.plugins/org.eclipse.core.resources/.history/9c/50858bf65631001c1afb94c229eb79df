package Day2;

public class GetImage {
	static String getReverse(String str) {
		String str3 = "";
		for (int i = 0; i < str.length(); i++) {
			str3 = str.charAt(i) + str3;

		}
		return str3;
	}

	public static void main(String[] args) {

		String str = "EARTH";
		String str2 = getReverse(str);
		System.out.println(str + "|" + str2);

	}

}
