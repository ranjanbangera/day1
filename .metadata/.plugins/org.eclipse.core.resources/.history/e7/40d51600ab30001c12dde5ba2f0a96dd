package Day2;

public class BubbleSort {
	static void bubbleSort(int[] arr) {
		for(int i=0;i<arr.length;i++) {
			for(int j=0;j<arr.length-i;j++) {
				int temp=arr[j];
				arr[j]=arr[j+1];
				arr[j+1]=temp;
			}
		}
		
	}
	static void printValue(int[] arr) {
		for(int i=0;i<arr.length;i++) {
			System.out.println(arr[i]+" ");
		}
	}
	

	public static void main(String[] args) {
		int[] arr= {1,8,3,4,6,5};
		bubbleSort(arr);
		printValue(arr);

	}

}
