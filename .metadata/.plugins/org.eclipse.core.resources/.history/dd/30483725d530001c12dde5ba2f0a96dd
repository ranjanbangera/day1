package Day2;

public class AlterString {
	static boolean IsVowel(char ch) {
		if (ch != 'a' && ch != 'e' && ch != 'i' && ch != 'o' && ch != 'u') {
			return false;
		}
		return true;
	}

	static String AlterStr(String str) {
		for (int i = 0; i < str.length(); i++) {
			if (!isVowel(str[i])) {
				if (str[i] == 'z') {
					s[i] = 'b';
				}
			}

		}
	}

	public static void main(String[] args) {

		String str = "Java";
		String str2 = alterStr(str);

	}

}
