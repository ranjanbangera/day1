package lab1;

public class FibonacciRecursive {
	static int n1=1,n2=1,n3;
	static void Fibonacci(int num) {
		if(num>0) {
			n3=n1+n2;
			n1=n2;
			n2=n3;
			System.out.print(" "+n3);
			Fibonacci(num-1);
		}
	}

	public static void main(String[] args) {
		int num=6;
		System.out.print(n1+" "+n2);
		Fibonacci(num-2);
		

	}

}
